<p id="notice"><%= notice %></p>

<h1>Listing Relacions</h1>

<table class="table table-bordered">
  <thead >
    <tr class="active">
      <th>Auto</th>
      <th>Servicio</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relacions.each do |relacion| %>
      <tr>
        <td><%= relacion.auto %></td>
        <td><%= relacion.servicio %></td>
        <td><%= relacion.estado %></td>
        <td><%= link_to 'Show', relacion %></td>
        <td><%= link_to 'Edit', edit_relacion_path(relacion) %></td>
        <td><%= link_to 'Destroy', relacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relacion', new_relacion_path %>
