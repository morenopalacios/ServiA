<p id="notice"><%= notice %></p>

<h1>Listing Tipodocs</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipodocs.each do |tipodoc| %>
      <tr>
        <td><%= tipodoc.nombre %></td>
        <td><%= link_to 'Show', tipodoc %></td>
        <td><%= link_to 'Edit', edit_tipodoc_path(tipodoc) %></td>
        <td><%= link_to 'Destroy', tipodoc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipodoc', new_tipodoc_path %>
